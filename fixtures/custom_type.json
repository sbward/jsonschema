{
  "$schema": "http://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/sbward/jsonschema/custom-type-field",
  "$ref": "#/$defs/CustomTypeField",
  "$defs": {
    "CustomTypeField": {
      "properties": {
        "CreatedAt": {
          "type": "string",
          "format": "date-time"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "CreatedAt"
      ]
    }
  }
}